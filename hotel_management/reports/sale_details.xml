<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="sale_report_inherit" inherit_id="sale.report_saleorder_document">
        <xpath expr="//div[@class='row mt32 mb32']" position="after">
            <div class="row mt32 mb32" id="tet inherit">
                <div t-if="doc.seller_name" class="col-auto col-3 mw-100 mb-2">
                    <strong>Seller Name:</strong>
                    <p class="m-0" t-field="doc.seller_name"/>
                </div>
                <div t-if="doc.new_quotation" class="col-auto col-3 mw-100 mb-2">
                    <strong>New Quotation:</strong>
                    <p class="m-0" t-field="doc.new_quotation"/>
                </div>
            </div>
        </xpath>

    </template>
</odoo>